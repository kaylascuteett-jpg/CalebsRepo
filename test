local H = Instance.new("Hint")
H.Parent = workspace

for i = 10, 1, -1 do
	H.Text = "You are using an unsupported executor. This script will not work.\n" ..
	         "A fix for ALL executors is coming soon. Stay tuned!\n" ..
	         "This message will be deleted in " .. i .. " seconds."
	task.wait(1)
end

H:Destroy()
